{
  "realm": "basyx",
  "enabled": true,
  "registrationAllowed": false,
  "rememberMe": true,
  "resetPasswordAllowed": true,
  "emailTheme": "keycloak",
  "loginTheme": "keycloak",
  "roles": {
    "realm": [
      { "name": "user", "description": "default user" },
      { "name": "admin", "description": "realm administrator" }
    ]
  },
  "users": [
    {
      "username": "the.admin",
      "enabled": true,
      "email": "admin@example.com",
      "firstName": "The",
      "lastName": "Admin",
      "emailVerified": true,
      "requiredActions": [],
      "credentials": [
        {
          "type": "password",
          "value": "BASYX_USER_PASSWORD_PLACEHOLDER",
          "temporary": false
        }
      ],
      "realmRoles": ["admin"]
    }
  ],
  "clients": [
    {
      "clientId": "CLIENTID_PLACEHOLDER",
      "secret": "CLIENTSECRET_PLACEHOLDER", 
      "name": "basyx-client",
